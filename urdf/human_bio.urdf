    <joint name="PelvisTransX" type="prismatic">
        <parent link="base"/>
        <child link="PelvisDummyTransX"/>
        <origin xyz="0 0 0"/> <!--Offset from PelvisBody-->
        <axis xyz="1 0 0"/>
        <limit effort="15" lower="-30" upper="30" velocity="5"/>
    </joint>
    <joint name="PelvisTransY" type="prismatic">
